<section class="py-5">
<div class="container px-5 text-center">
  <h1>Edit current job listing</h1>
    <div class="row justify-content-center text-start mb-4">
      <div class="col-6">
        <%= render 'form', job: @job %>

        <%= link_to 'Show', job_path(@job) %>
          <div class="form-group width-50">

            <%= button_to 'Back', :back, method: :get, class: 'form-control' %>
          </div>
      </div>
    </div>
</div>
</section>
